{
  "name": "@react-spring/shared",
  "version": "9.0.0-rc.3",
  "description": "Globals and shared modules",
  "main": "src/index.ts",
  "scripts": {
    "build": "tsc -p . --outDir dist/cjs && tsc -p . --module ES6 --outDir dist/esm && cherry-pick --cwd=dist ../src --cjs-dir=cjs --esm-dir=esm"
  },
  "publishConfig": {
    "directory": "dist"
  },
  "dependencies": {
    "@react-spring/types": "link:../types",
    "fluids": "^0.1.6",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "cherry-pick": "^0.5.0",
    "typescript": "3.8.3"
  }
}
